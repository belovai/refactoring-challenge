includes:
    - vendor/larastan/larastan/extension.neon

parameters:

    paths:
        - app

    # The level 8 is the highest level
    level: 6

    ignoreErrors:
        - '#Property [a-zA-Z0-9\\_]+::\$[a-zA-Z0-9\\_]+ is never read, only written.#'

    excludePaths:

    reportUnmatchedIgnoredErrors: false
